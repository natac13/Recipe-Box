{
  "name": "recipebox",
  "version": "0.1.0",
  "description": "Recipe Box React Challenge from Free Code Camp",
  "main": "build/index.js",
  "scripts": {
    "clean": "rimraf build/",
    "start": "cross-env NODE_ENV=development nodemon -r babel-register -r babel-polyfill -r dotenv/config server/development.js",
    "build:app": "webpack -p --config ./webpack.production.config.js --progress --color",
    "copyFavicon": "cp favicon.ico ./build/favicon.ico",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production npm run build:app && npm run copyFavicon",
    "predeploy": "npm run build",
    "deploy": "rimraf docs/ && cp -r build/ docs/",
    "lint": "eslint app/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/natac13/Recipe-Box.git"
  },
  "author": "Sean Campbell <sean.campbell13@gmail.com> (https://seancampbellnatac.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/natac13/Recipe-Box/issues"
  },
  "homepage": "https://github.com/natac13/Recipe-Box#readme",
  "devDependencies": {
    "autoprefixer": "7.2.3",
    "babel-core": "6.26.0",
    "babel-eslint": "8.1.0",
    "babel-loader": "7.1.2",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.24.1",
    "babel-register": "6.26.0",
    "cross-env": "5.1.3",
    "css-loader": "0.28.7",
    "dsv-loader": "2.0.0",
    "eslint": "4.14.0",
    "eslint-config-natac13": "1.3.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "html-webpack-plugin": "2.30.1",
    "node-sass": "4.7.2",
    "nodemon": "1.14.3",
    "normalize.css": "7.0.0",
    "postcss": "6.0.14",
    "postcss-loader": "2.0.9",
    "rimraf": "2.6.2",
